@page
@model IT_Center_Website.Pages.Admin.Accounts.EditModel
@{
	Layout = "~/Pages/Admin/_AdminLayout.cshtml";
}
<body>
	<h1>Modify Account</h1>
	<form method="post">
		<div>
			<label asp-for="Account.Id"></label>
			<input asp-for="Account.Id" disabled />
		</div>
		<div>
			<label asp-for="Account.Username"></label>
			<input asp-for="Account.Username" disabled />
		</div>
		<div>
			<label asp-for="Account.Email"></label>
			<input asp-for="Account.Email" disabled />
		</div>
		<div>
			<label asp-for="Account.Role"></label>
			<input asp-for="Account.Role" disabled />
		</div>
		<div>
			<button type="submit">Modify</button>
		</div>
	</form>
</body>
